<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net7.0-windows</TargetFramework>
		<Nullable>enable</Nullable>
		<UseWindowsForms>true</UseWindowsForms>
		<NeutralLanguage>en-US</NeutralLanguage>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="Properties\Resources.en.restext" />
	  <None Remove="transforms\trial-balance.xslt" />
	</ItemGroup>

	<ItemGroup>
	  <Content Include="transforms\trial-balance.xslt">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Include="Properties\Resources.en.restext" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="MessagePack" Version="2.5.124" />
	  <PackageReference Include="Microsoft.Web.WebView2" Version="1.0.1905-prerelease" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Liber.Core\Liber.Core.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Update="Accounts\AccountForm.cs">
	    <SubType>Form</SubType>
	  </Compile>
	  <Compile Update="Accounts\AccountsForm.cs">
	    <SubType>Form</SubType>
	  </Compile>
	  <Compile Update="Accounts\EditAccountForm.cs">
	    <SubType>Form</SubType>
	  </Compile>
	  <Compile Update="Accounts\NewAccountForm.cs">
	    <SubType>Form</SubType>
	  </Compile>
	  <Compile Update="Companies\CompanyForm.cs">
	    <SubType>Form</SubType>
	  </Compile>
	  <Compile Update="Companies\EditCompanyForm.cs">
	    <SubType>Form</SubType>
	  </Compile>
	  <Compile Update="Companies\NewCompanyForm.cs">
	    <SubType>Form</SubType>
	  </Compile>
	  <Compile Update="Controls\ListViewEx.cs">
	    <SubType>Component</SubType>
	  </Compile>
	  <Compile Update="Properties\Resources.Designer.cs">
	    <DependentUpon>Resources.resx</DependentUpon>
	    <DesignTime>True</DesignTime>
	    <AutoGen>True</AutoGen>
	  </Compile>
	  <Compile Update="Properties\Settings.Designer.cs">
	    <DesignTimeSharedInput>True</DesignTimeSharedInput>
	    <AutoGen>True</AutoGen>
	    <DependentUpon>Settings.settings</DependentUpon>
	  </Compile>
	  <Compile Update="SettingsForm.cs">
	    <SubType>Form</SubType>
	  </Compile>
	  <Compile Update="UrlForm.cs">
	    <SubType>Form</SubType>
	  </Compile>
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Update="Properties\Resources.resx">
	    <LastGenOutput>Resources.Designer.cs</LastGenOutput>
	    <Generator>ResXFileCodeGenerator</Generator>
	  </EmbeddedResource>
	</ItemGroup>

	<ItemGroup>
	  <None Update="Properties\Settings.settings">
	    <Generator>SettingsSingleFileGenerator</Generator>
	    <LastGenOutput>Settings.Designer.cs</LastGenOutput>
	  </None>
	</ItemGroup>

</Project>